      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#c4df9d"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation, constant.other.symbol",
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "String Escape",
